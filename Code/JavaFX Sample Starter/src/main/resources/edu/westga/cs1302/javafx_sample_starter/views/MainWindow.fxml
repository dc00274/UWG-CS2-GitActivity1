<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="840.0" prefWidth="557.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.westga.cs1302.javafx_sample_starter.views.MainWindow">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <Label text="Label" />
      <TextField fx:id="textField" />
      <TextArea fx:id="textArea" prefHeight="200.0" prefWidth="200.0" />
      <ComboBox fx:id="comboBox" prefWidth="150.0" />
      <Button fx:id="button" mnemonicParsing="false" onAction="#onSubmit" text="Button" />
      <ListView fx:id="listView" prefHeight="200.0" prefWidth="200.0" />
      <Button fx:id="removeButton" mnemonicParsing="false" onAction="#removeTask" text="Button" />
      <TextField fx:id="descriptionText" editable="false" />
      <Button fx:id="updateButton" mnemonicParsing="false" onAction="#updateDescription" text="Button" />
      <TextArea fx:id="descriptionPriority" prefHeight="200.0" prefWidth="200.0" />
      <Label fx:id="numberTasks" text="Label" />
      <Button fx:id="displayNumber" mnemonicParsing="false" onAction="#taskNumber" text="Button" />
   </children>
</VBox>
